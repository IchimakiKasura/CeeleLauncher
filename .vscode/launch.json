{
    // If you have C# Dev Kit, then you don't need these for VSCODE
    "version": "0.2.0",
    "configurations": [
        {
            "name": "DEBUG",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/bin/Debug/net7.0-windows/CeeleLauncher.exe",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "enableStepFiltering": false,
            "justMyCode": false,
            "logging": {
                "engineLogging": false,
                "moduleLoad": false,
                "threadExit": false
            }
        }
    ]
}